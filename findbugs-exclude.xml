<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>

    <Match>
        <Class name="io.kcache.keta.server.KetaMain"/>
        <Bug pattern="DM_EXIT"/>
    </Match>
    <Match>
        <Class name="io.kcache.keta.utils.PemUtils"/>
        <Bug pattern="DM_DEFAULT_ENCODING"/>
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Bug pattern="MS_EXPOSE_REP"/>
    </Match>
    <!-- exclude generated code -->
    <Match>
        <Class name="~io.etcd.jetcd.api.*"/>
    </Match>
    <Match>
        <Class name="~io.kcache.keta.pb.*"/>
    </Match>

</FindBugsFilter>
